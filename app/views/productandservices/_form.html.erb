<%= simple_form_for(@productandservice) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :description %>
    <%= f.input :sourcing_and_fulfillment %>
    <%= f.input :technology_used %>
    <%= f.input :expansion_plans %>
    <%= f.association :business_plan %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
