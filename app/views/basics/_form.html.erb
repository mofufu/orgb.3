<%= simple_form_for(@basic) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :value_proposition %>
    <%= f.input :objective %>
    <%= f.input :plan_description %>
    <%= f.association :marketing_plan %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
