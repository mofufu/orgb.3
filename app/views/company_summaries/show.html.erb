<%- model_class = CompanySummary -%>
<div class="page-header">
  <h1><%=t '.title', :default => CompanySummary %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:ownership) %></dt>
      <dd><pre class="prettyprint"><%= @company_summary.ownership %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:start_plan) %></dt>
      <dd><pre class="prettyprint"><%= @company_summary.start_plan %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:locations) %></dt>
      <dd><pre class="prettyprint"><%= @company_summary.locations %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              company_summaries_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_company_summary_path(@company_summary), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              company_summary_path(@company_summary),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

